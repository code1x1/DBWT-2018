@model IEnumerable<emensa.Models.Student>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Studiengang)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Matrikelnummer)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FkFhangeNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Studiengang)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Matrikelnummer)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FkFhangeNavigation.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.StudentId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.StudentId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.StudentId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
